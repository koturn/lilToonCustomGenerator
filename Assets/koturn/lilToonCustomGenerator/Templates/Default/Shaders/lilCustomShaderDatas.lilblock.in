ShaderName "%%SHADER_NAME%%"
EditorName "%%NAMESPACE%%.%%INSPECTOR_NAME%%"
!!ifnotempty:SHOULD_EMIT_GEOMETRY_SHADER!!
Replace "\r\n" "\n"
Replace "\r" "\n"
Replace "// Build Option\n            #pragma vertex vert\n            #pragma fragment frag\n" "// Build Option\n            #pragma vertex vert\n            #pragma geometry geomCustom\n            #pragma fragment frag\n            #pragma require geometry\n            #define USE_CUSTOM_GEOMETRY\n"
Replace "// Build Option\n            #pragma vertex vertTess\n            #pragma fragment frag\n            #pragma hull hull\n            #pragma domain domain\n            #pragma require tesshw tessellation\n" "// Build Option\n            #pragma vertex vertTess\n            #pragma fragment frag\n            #pragma hull hullOpt\n            #pragma domain domainOpt\n            #pragma geometry geomCustom\n            #pragma require tesshw tessellation geometry\n            #define USE_CUSTOM_GEOMETRY\n"
!!  ifnotempty:OVERRIDE_FUR_GEOMETRY!!
!!    ifnotempty:OVERRIDE_ONEPASS_GEOMETRY!!
Replace "// Build Option\n            #pragma vertex vert\n            #pragma geometry geom\n            #pragma fragment frag\n" "// Build Option\n            #pragma vertex vert\n            #pragma geometry geomCustom\n            #pragma fragment frag\n            #pragma require geometry\n"
!!    else!!
Replace "Fur" "// Build Option\n            #pragma vertex vert\n            #pragma geometry geom\n            #pragma fragment frag\n" "// Build Option\n            #pragma vertex vert\n            #pragma geometry geomCustom\n            #pragma fragment frag\n            #pragma require geometry\n"
!!    endif!!
!!  elifnotempty:OVERRIDE_ONEPASS_GEOMETRY!!
Replace "!Fur" "// Build Option\n            #pragma vertex vert\n            #pragma geometry geom\n            #pragma fragment frag\n" "// Build Option\n            #pragma vertex vert\n            #pragma geometry geomCustom\n            #pragma fragment frag\n            #pragma require geometry\n"
!!  endif!!
!!endif!!
